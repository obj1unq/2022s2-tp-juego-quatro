import wollok.game.*
import niveles.*

program juego {
	const emkay = game.sound("emkay.ogg")
	emkay.shouldLoop(true)
	emkay.volume(1)
	game.schedule(100, {emkay.play()})
	
	game.title("Quatro")
	game.boardGround("tablero.png")

	game.width(40)
	game.height(24)
	game.cellSize(35)
	
	nivel1.iniciar()
	
	game.start()
}
